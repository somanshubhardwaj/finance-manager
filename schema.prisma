generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Transaction {
  id       Int     @id @default(autoincrement())
  amount   Float
  title    String
  remark   String? @default("")
  category String

  userId Int
}
